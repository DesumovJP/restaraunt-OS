{
  "kind": "collectionType",
  "collectionName": "suppliers",
  "info": {
    "singularName": "supplier",
    "pluralName": "suppliers",
    "displayName": "Supplier"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "contactName": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "phone": {
      "type": "string"
    },
    "address": {
      "type": "text"
    },
    "taxId": {
      "type": "string"
    },
    "isActive": {
      "type": "boolean",
      "default": true
    },
    "ingredients": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::ingredient.ingredient",
      "mappedBy": "suppliers"
    }
  }
}
