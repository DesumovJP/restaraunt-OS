{
  "kind": "collectionType",
  "collectionName": "tables",
  "info": {
    "singularName": "table",
    "pluralName": "tables",
    "displayName": "Table"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "number": {
      "type": "integer",
      "required": true,
      "unique": true
    },
    "capacity": {
      "type": "integer",
      "default": 4
    },
    "status": {
      "type": "enumeration",
      "enum": ["free", "occupied", "reserved"],
      "default": "free"
    },
    "currentGuests": {
      "type": "integer"
    },
    "reservedBy": {
      "type": "string"
    },
    "reservedAt": {
      "type": "datetime"
    },
    "occupiedAt": {
      "type": "datetime"
    },
    "freedAt": {
      "type": "datetime"
    },
    "lastSessionDurationMs": {
      "type": "biginteger"
    },
    "zone": {
      "type": "string"
    },
    "isActive": {
      "type": "boolean",
      "default": true
    },
    "lastCloseReason": {
      "type": "enumeration",
      "enum": ["mistaken_open", "no_show", "walkout", "technical_error", "normal"]
    },
    "closeComment": {
      "type": "text"
    }
  }
}
